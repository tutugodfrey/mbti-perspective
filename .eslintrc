{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-prototype-builtins": "off",
        "arrow-parens-no-return-assign": "off"
    }
}
